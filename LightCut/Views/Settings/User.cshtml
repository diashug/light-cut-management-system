
@{
    ViewData["Title"] = "Gestão de Utilizadores";
}

<div id="usersApp">
    <div class="container top-space">
        <div id="wrapper">
            <div id="page-wrapper">
                <div class="row">
                    <div class="col-md-12">
                        <h1 class="page-header">@ViewData["Title"]</h1>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <h1>Utilizadores</h1>
                        <div class="table-padding">
                            <table id="usersTable" class="table table-bordered">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Nome</th>
                                        <th>Username</th>
                                        <th>Grupo de Acesso</th>
                                        <th>Sub-Grupo de Acesso</th>
                                        <th>Observações</th>
                                        <th>Opções</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="bottom-space"></div>
    <footer id="footer" class="navbar navbar-default navbar-fixed-bottom footer">
    </footer>
</div>
<script src="~/js/settings/users/index.js" type="text/javascript"></script>


